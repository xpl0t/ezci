{
  "name": "ezci",
  "version": "1.0.2",
  "description": "Easy CI is a CLI to trigger pipelines for rappid continuous integration",
  "scripts": {
    "build": "tsc",
    "start": "node bin/ezci.js",
    "lint": "eslint '{bin,commands}/**/*.ts' --fix",
    "publish": "npm run publish:latest",
    "publish:latest": "npm publish --tag latest",
    "publish:next": "npm publish --tag next"
  },
  "bin": {
    "ezci": "bin/ezci.js"
  },
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpl0t/ezci.git"
  },
  "keywords": [
    "cli",
    "cicd",
    "pipeline"
  ],
  "author": "Johannes Weih",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpl0t/ezci/issues"
  },
  "homepage": "https://github.com/xpl0t/ezci#readme",
  "dependencies": {
    "@caporal/core": "^2.0.2",
    "chalk": "^5.0.0",
    "inquirer": "^8.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/node": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "typescript": "^4.5.4"
  }
}
